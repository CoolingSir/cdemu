#!/bin/sh
# prerm script for vhba-dkms
#

#DEBHELPER#

myerror ()
{
	echo "ERROR: $1" >&2
	exit 1
}

mywarning ()
{
	echo "WARNING: $1" >&2
}

case "$1" in
	remove|upgrade|deconfigure)
		# Remove the cdrom group
		# delgroup --only-if-empty cdrom || mywarning "failed to remove group 'cdrom'."
	;;

	failed-upgrade)
	;;

	*)
		myerror "prerm called with unknown argument '$1'."
	;;
esac

exit 0

