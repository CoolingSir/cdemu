<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-cif">
        <sub-class-of type="application/octet-stream"/>

        <_comment>Adaptec Easy CD/DVD Creator image file</_comment>

        <glob pattern="*.cif"/>
        <magic priority="60">
            <match value="RIFF\0\0\0\0imag" mask="0xffffffff00000000ffffffff" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
