<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-xmd">
        <sub-class-of type="application/octet-stream"/>

        <_comment>GameJack image file (*.xmd)</_comment>

        <glob pattern="*.xmd"/>

        <magic priority="80">
             <match value="MEDIA DESCRIPTOR\x01" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
