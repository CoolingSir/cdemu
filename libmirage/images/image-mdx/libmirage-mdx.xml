<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-mdx">
        <sub-class-of type="application/octet-stream"/>

        <comment>DaemonTools image file</comment>

        <glob pattern="*.mds"/>
        <glob pattern="*.mdx"/>

        <magic priority="80">
             <match value="MEDIA DESCRIPTOR\x02" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
