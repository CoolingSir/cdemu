<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-cif">
        <sub-class-of type="application/octet-stream"/>

        <comment>Adaptec Easy CD/DVD Creator image file</comment>

        <glob pattern="*.cif"/>
        <magic priority="50">
            <match value="RIFF" type="string" offset="0"/>
            <match value="imag" type="string" offset="8"/>
        </magic>
    </mime-type>
</mime-info>
