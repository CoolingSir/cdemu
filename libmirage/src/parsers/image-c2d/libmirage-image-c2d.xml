<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/libmirage-c2d">
        <sub-class-of type="application/libmirage-image"/>
        <sub-class-of type="application/octet-stream"/>

        <alias type="application/x-c2d"/>
        
        <comment>Roxio Easy Media Creator image file</comment>
        
        <glob pattern="*.c2d"/>

        <magic priority="50">
             <match value="Roxio Image File Format 3.0" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
