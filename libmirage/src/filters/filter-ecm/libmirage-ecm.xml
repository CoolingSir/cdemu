<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-ecm">
        <sub-class-of type="application/octet-stream"/>

        <comment>ECM'ified image file</comment>

        <glob pattern="*.ecm"/>

        <magic priority="50">
            <match value="ECM\x00" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
