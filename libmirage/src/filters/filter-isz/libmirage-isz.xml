<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-isz">
        <sub-class-of type="application/octet-stream"/>

        <comment>Compressed ISO image file</comment>

        <glob pattern="*.isz"/>

        <magic priority="50">
            <match value="IsZ!" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
