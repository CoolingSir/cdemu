<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-daa">
        <sub-class-of type="application/octet-stream"/>

        <_comment>PowerISO image file</_comment>

        <glob pattern="*.daa"/>

        <magic priority="50">
            <match value="DAA" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
