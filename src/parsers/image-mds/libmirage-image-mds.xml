<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/libmirage-mds">
        <sub-class-of type="application/libmirage-image"/>
        <sub-class-of type="application/octet-stream"/>

        <alias type="application/x-mds"/>
        
        <comment>Alcohol 120% image file</comment>
        
        <glob pattern="*.mds"/>
        <glob pattern="*.xmd"/>

        <magic priority="50">
             <match value="MEDIA DESCRIPTOR" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
