<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/libmirage-cif">
        <sub-class-of type="application/libmirage-image"/>
        <sub-class-of type="application/octet-stream"/>

        <alias type="application/x-cif"/>
        
        <comment>Adaptec Easy CD/DVD Creator image file</comment>
        
        <glob pattern="*.cif"/>

        <magic priority="50">
             <match value="RIFF" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
